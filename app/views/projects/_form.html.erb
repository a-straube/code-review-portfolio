<%= render "layouts/errors", :object => @project %>

<%= form_for [@skill, @project] do |f| %>
  <p><%= f.label :name %></p>
  <p><%= f.text_field :name %></p>
  <p><%= f.label :description %></p>
  <p><%= f.text_field :description %></p>
  <p><%= f.label "GitHub URL" %></p>
  <p><%= f.text_field :git_url %></p>
  <p><%= f.submit %></p>
<% end %>
